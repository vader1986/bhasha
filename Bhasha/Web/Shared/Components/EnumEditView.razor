@typeparam T

<MudSelect T="string" Label="@Label" Value="@_selectedValue" ValueChanged="@OnValueChanged">
    @foreach(var expressionType in EnumValues)
    {
        <MudSelectItem Value="@expressionType">
            @expressionType
        </MudSelectItem>
    }
    <MudSelectItem T="string" Value="@NoSelection">None</MudSelectItem>
</MudSelect>