<MudIconButton 
    Icon="@Icons.Material.TwoTone.Audiotrack" 
    OnClick="@PlayAudioAsync" 
    Disabled="@Disabled"
    Variant="Variant.Filled"/>

@if (_audioFileName is not null)
{
    <audio id="sound" src="@_audioFileName">
        Your browser does not support the audio element.
    </audio>
}